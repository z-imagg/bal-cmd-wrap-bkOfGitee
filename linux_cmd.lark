// grammar single_cmd

start: single_cmd

///////////非终结符
single_cmd: program av_pairs*

program: FILE_NAME

av_pairs: av_pair+

av_pair: arg argv?


arg: "-" ("-")? (L|D) ("_"|L|D)*
argv: TK |   _STR


///////////////终结符
TK: ("_"|L|D)+

D: "0".."9"
LL: "a".."z"
UL: "A".."Z"
L: LL | UL
W: L+

_STR : _STR_DQ | _STR_SQ
_STR_INNER: /.*?/
_STR_DQ : "\"" _STR_INNER "\""
_STR_SQ : "\'" _STR_INNER "\'"

FILE_NAME: (FILE_NAME_LEAD | D | L) (FILE_NAME_MID | D | L)+
FILE_NAME_LEAD: "." | "_" | "," | "+"
FILE_NAME_MID: FILE_NAME_LEAD | "-"



///////忽略空格等
WS_INLN: (" "|/\t/)+
%ignore WS_INLN
